{
  "expo": {
    "name": "Silent Bridge",
    "slug": "silent-bridge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "scheme": "silentbridge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jona0002.silentbridge"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "src/assets/images/favicon.png",
      "name": "Silent Bridge",
      "shortName": "Silent Bridge",
      "description": "Connect deaf users with sign language interpreters",
      "themeColor": "#2559bd",
      "backgroundColor": "#ffffff",
      "publicPath": "public"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "src/assets/images/favicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "af6f0e3a-0123-4444-a13c-a4333a77a819"
      }
    }
  }
}
